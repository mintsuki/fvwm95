Style "Ghostview" 	TitleIcon mini-gv.xpm
Style "gv*"	 	TitleIcon mini-gv.xpm
Style "Xcolorsel" 	TitleIcon mini-colors.xpm
Style "IDL*"      	TitleIcon mini-zoom.xpm
Style "xeyes"     	TitleIcon mini-eyes.xpm
